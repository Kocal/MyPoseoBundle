{
    "name": "tristanbes/my-poseo-bundle",
    "description": "Access to MyPoseo API",
    "keywords": ["seo", "myposeo", "google", "rank"],
    "homepage": "https://github.com/tristanbes/MyPoseoBundle",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Tristan Bessoussa",
            "email": "tristan.bessoussa@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.4",
        "psr/cache": "^1.0",
        "symfony/config": "~3.0|^4.0",
        "symfony/http-kernel": "~3.0|^4.0",
        "symfony/dependency-injection": "~3.0|^4.0",
        "doctrine/doctrine-cache-bundle": "~1.0",
        "php-http/httplug": "^1.0",
        "php-http/client-implementation": "^1.0",
        "php-http/message": "^1.0",
        "php-http/discovery": "^1.0",
        "php-http/client-common": "^1.0"
    },
    "require-dev": {
        "php-http/guzzle6-adapter": "^1.0"
    },
    "autoload": {
        "psr-4": { "Tristanbes\\MyPoseoBundle\\": "" }
    },
    "minimum-stability": "stable",
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
